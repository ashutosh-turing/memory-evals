# Memory-Break Orchestrator Configuration

# Application Settings
APP_NAME="Memory-Break Orchestrator"
APP_VERSION="0.1.0"
DEBUG=false
HOST=127.0.0.1
PORT=8000

# Database Configuration
DATABASE_URL=postgresql+psycopg2://user:password@localhost:5432/memory_break_db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# File Storage
RUN_ROOT=~/MemEval
MAX_FILES_PER_TASK=50

# Agent CLI Binaries
IFLOW_BIN=iflow
CLAUDE_BIN=claude
GEMINI_BIN=gemini

# API Keys for LLM Judge (Optional)
# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here
# GOOGLE_API_KEY=your_google_key_here

# iFlow Authentication (Required for iFlow agent)
IFLOW_API_KEY=sk-f47abafee52004606b27e9984ee52eb3
IFLOW_BASE_URL=https://apis.iflow.cn/v1
IFLOW_MODEL_NAME=qwen3-coder-plus

# Security
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8000","http://127.0.0.1:8000"]

# Task Processing
TASK_TIMEOUT_SECONDS=3600
AGENT_SESSION_TIMEOUT=1800

# Compression Detection
COMPRESSION_THRESHOLD_LOW=30
COMPRESSION_JUMP_THRESHOLD=30

# Judge Configuration
DEFAULT_JUDGE=heuristic
JUDGE_MODEL=gpt-4

# Prompt Generation Configuration
USE_GPT_PROMPTS=true
PROMPT_MODEL=gpt-4o
PROMPT_TEMPERATURE=0.3
PROMPT_MAX_TOKENS=4000
